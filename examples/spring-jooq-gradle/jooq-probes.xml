<?xml version="1.0" encoding="UTF-8" ?>
<!--<!DOCTYPE jfragent SYSTEM-->
<!--        "https://raw.githubusercontent.com/openjdk/jmc/master/agent/src/main/resources/org/openjdk/jmc/agent/impl/jfrprobes_schema.xsd">-->
<jfragent>
    <config>
        <classprefix>__JFREvent</classprefix>
        <allowtostring>true</allowtostring>
        <allowconverter>true</allowconverter>
    </config>
    <events>
        <event id="jooq.ExecutePreparedStmt">
            <label>PreparedStatement Execute</label>
            <class>org.jooq.tools.jdbc.DefaultPreparedStatement</class>
            <description>Emitted when JOOQ executes a statement</description>
            <path>JDBC</path>
            <stacktrace>false</stacktrace>
            <rethrow>false</rethrow>
            <location>WRAP</location>
            <method>
                <name>execute</name>
                <descriptor>()Z</descriptor>
            </method>
        </event>
        <event id="jooq.AbstractQuerySql">
            <label>jOOQ Execute Statement with SQL</label>
            <class>org.jooq.impl.AbstractResultQuery</class>
            <description>Emitted when jOOQ executes a statement, may contain the rendered SQL</description>
            <path>JDBC</path>
            <stacktrace>false</stacktrace>
            <rethrow>false</rethrow>
            <location>WRAP</location>
            <method>
                <name>execute</name>
                <descriptor>(Lorg/jooq/ExecuteContextLorg/jooq/ExecuteListener;)I</descriptor>
<!--                <parameters>-->
<!--                    <parameter index="0">-->
<!--                        <name>SQL</name>-->
<!--                        <description>Rendered SQL String, may be null</description>-->
<!--                        <contenttype>None</contenttype>-->
<!--                        <converter>org.moditect.jfrunit.demos.spring_jooq.ExecuteContextSqlConverter</converter>-->
<!--                    </parameter>-->
<!--                </parameters>-->
            </method>
        </event>
    </events>
</jfragent>
