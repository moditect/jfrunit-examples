<jfragent>
    <config>
        <classprefix>__JFREvent</classprefix>
        <allowtostring>true</allowtostring>
        <allowconverter>true</allowconverter>
    </config>
    <events>
        <event id="jooq.ExecutePreparedStmt">
            <label>PreparedStatement Execute</label>
            <class>org.jooq.tools.jdbc.DefaultPreparedStatement</class>
            <description>Emitted when JOOQ executes a statement</description>
            <path>JDBC</path>
            <stacktrace>false</stacktrace>
            <rethrow>false</rethrow>
            <location>WRAP</location>
            <method>
                <name>execute</name>
                <descriptor>()Z</descriptor>
            </method>
        </event>
    </events>
</jfragent>
